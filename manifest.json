{
  "name": "Alias Url",
  "description": "Alias Url ",
  "version": "0.0.1",
  "background": {
    "persistent": false,
    "scripts": [
      "src/fuse.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_security_policy": "script-src 'self' object-src 'self'",
  "options_page": "options.html",
  "omnibox": {
    "keyword": "as"
  },
  "manifest_version": 2
}

