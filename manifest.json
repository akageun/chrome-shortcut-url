{
  "name": "Shortcut URL",
  "description": "Shortcut url",
  "version": "0.0.1",
  "background": {
    "persistent": false,
    "scripts": [
      "src/js/lib/fuse.js",
      "src/js/background.js"
    ]
  },
  "icons": {
    "16": "src/img/icon-16.png",
    "48": "src/img/icon-48.png",
    "128": "src/img/icon-128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "downloads"
  ],
  "content_security_policy": "script-src 'self'; default-src 'self'",
  "web_accessible_resources": [
    "src/css/*.css"
  ],
  "options_page": "src/views/options.html",
  "omnibox": {
    "keyword": "as"
  },
  "manifest_version": 2
}

