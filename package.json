{
  "name": "chrome-shortcut-url",
  "version": "0.0.2",
  "description": "With the this chrome extension, you can quick URL searches with shortcuts.",
  "author": "akageun <akageun@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch"
  },
  "dependencies": {
    "vuedraggable": "^2.20.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "fuse.js": "^3.4.4",
    "jquery": "^3.3.1",
    "popper.js": "^1.15.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@types/chrome": "^0.0.74",
    "archiver": "^3.0.0",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "ejs": "^2.6.1",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.4",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.20.2",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.1.2"
  }
}
